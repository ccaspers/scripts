[core]
	editor = nano
[credential]
	helper = cache --timeout=3600
[alias]
	pom = push origin master
	st = status
	co = checkout
	ci = commit
	br = branch
	df = diff
	lp = log -p
	fe = fetch origin
	hr = reset origin/master --hard
	me = config user.name
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
